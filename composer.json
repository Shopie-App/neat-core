{
    "name": "shopie/neat-core",
    "description": "Neat, the PHP 8 API for microservices",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Shopie",
            "email": "support@shopie.gr"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:Shopie-App/di-container.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Neat\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Neat\\Tests\\": "tests/"
        }
    },
    "require": {
        "shopie/di-container": "^1.0"
    },
    "require-dev": {
        "firebase/php-jwt": "^6.8.1"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
